<div class="stockists">
  <div class="page-width">
    <h6 class="title">{{section.settings.title}}</h6>
    <div class="content_map">
      {{section.settings.content}}
      {%comment%}
      <div class="contents">
        <div class="search">
          <input type="text" id="fsearch" name="fsearch">
          <button class="icon_search"><i class="fas fa-search"></i></button>
        </div>
        <div class="list_map">

          {% for item in section.blocks %}
          <div class="item">
             <i class="fas fa-map-marker-alt"></i>
             <div class="item_content">
               <h6>{{item.settings.titles}}</h6>
               <p>{{item.settings.subtitle1}}</p>
               <p>{{item.settings.subtitle2}}</p>
               <p>{{item.settings.subtitle3}}</p>
               <p>{{item.settings.subtitle4}}</p>
               <p>{{item.settings.subtitle5}}</p>
               <p>{{item.settings.subtitle6}}</p>
               <p>{{item.settings.subtitle7}}</p>
             </div>
          </div>
          {% endfor %}
        </div>
        <p class="copy_right">{{section.settings.copy_right}}</p>
      </div>
      <div class="map_wrap">
        {{ page.content }}
      </div>
      {%endcomment%}
    </div>
  </div>
</div>

